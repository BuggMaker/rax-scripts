<template name="element">
  <view
    style="{{r.style}}"
    class="h5-{{r.tagName}} {{r.className}}"
    id="{{r.id}}"
    data-private-node-id="{{r.nodeId}}"
    data-private-page-id="{{r.pageId}}"

    onTap="onTap"
  >
    <block a:for="{{item.children}}" a:key="nodeId">
      <block a:if="{{item.type === 'text'}}">
        {{item.content}}
      </block>
      <block a:else>
        <template is="element" data="{{r: item}}" />
      </block>
      <block>
    </block>
  </view>
</template>
